{
  "kind": "collectionType",
  "collectionName": "residents",
  "info": {
    "singularName": "resident",
    "pluralName": "residents",
    "displayName": "Resident"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "resident"
    },
    "profile_resident": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::profile-resident.profile-resident",
      "inversedBy": "resident"
    },
    "resident_programs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resident-program.resident-program",
      "mappedBy": "resident"
    }
  }
}
