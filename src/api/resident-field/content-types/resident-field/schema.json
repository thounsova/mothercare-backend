{
  "kind": "collectionType",
  "collectionName": "resident_fields",
  "info": {
    "singularName": "resident-field",
    "pluralName": "resident-fields",
    "displayName": "Resident_Field"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "activity_date": {
      "type": "date"
    },
    "program": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program.program",
      "inversedBy": "resident_fields"
    },
    "activity_upload": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "comments": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
