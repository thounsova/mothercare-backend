{
  "kind": "collectionType",
  "collectionName": "resident_relatives",
  "info": {
    "singularName": "resident-relative",
    "pluralName": "resident-relatives",
    "displayName": "resident_relative"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true
    },
    "profile_resident": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::profile-resident.profile-resident",
      "inversedBy": "resident_relatives"
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Man",
        "Woman"
      ]
    },
    "resident_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::resident-typ.resident-typ",
      "inversedBy": "resident_relatives"
    },
    "date": {
      "type": "date"
    }
  }
}
